<!DOCTYPE html>
<html lang="en">

<head>
  <title>Map</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
  <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
  <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

  <!-- Leaflet stuff for maps below -->
  <%- include('partials/_leafletApiLinks') %>
    <!-- Leaflet stuff for maps above -->

    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>
</head>

<body>
  <%- include('partials/_header') %>
    <section class="body-container">
      <span class="section-title"> Take a look at map: <%= JSON.stringify(parksArray[0].map_name) %> </span>
      <div id="mapid"></div>
    </section>
</body>
<!-- Leaflet stuff for maps below -->
<script script type="text/javascript" src="/scripts/map_idScripts.js"></script>
<script>
    const parks = <%- JSON.stringify(parksArray) %>;
    parks.forEach(park => {
      lat = park.coordinates_lat;
      lon = park.coordinates_long;
      L.marker([lat,lon]).addTo(mymap);
    });
</script>
<!-- Leaflet stuff for maps above -->

</html>
